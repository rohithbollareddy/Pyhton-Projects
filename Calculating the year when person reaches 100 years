name= input('Enter your name: ')
age= int(input('Enter your age: '))
print('Your name is :',name)
print('Your age is :',age)
X=100-age
current_year= 2021
if age<100:
    C=current_year+X
    print('The year you will reach 100 is: ',C)
else:
    print('You are above 100 years of age.Please try below 100!!!')
